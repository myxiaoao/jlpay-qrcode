{
    "name": "cooper/jlpay-qrcode",
    "description": "JiaLian Payment PHP SDK.",
    "keywords": [
        "cooper",
        "jlpay"
    ],
    "homepage": "https://github.com/myxiaoao/jlpay-qrcode",
    "license": "MIT",
    "authors": [
        {
            "name": "Cooper",
            "email": "myxiaoao@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "monolog/monolog": "^3.6",
        "ext-openssl": "*"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Cooper\\JlPayQrcode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cooper\\JlPayQrcode\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
